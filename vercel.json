{
    "version": 2,
    "builds": [
      {
        "src": "man_jewellers/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb","runtime":"python3.10.13"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {"distDir": "staticfiles_build","runtime":"python3.10.13"}
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "man_jewellers/wsgi.py"
      }
    ]
  }